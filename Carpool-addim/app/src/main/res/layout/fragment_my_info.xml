<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.MyInfoFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/my_info_img"
            android:src="@drawable/picture"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text1"
            android:textSize="20dp"
            android:id="@+id/my_info_text1"
            android:layout_alignTop="@+id/my_info_img"
            android:layout_toEndOf="@+id/my_info_img"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textSize="20dp"
            android:text="@string/my_info_text2"
            android:id="@+id/my_info_text"
            android:layout_below="@+id/my_info_text1"
            android:layout_toEndOf="@+id/my_info_img"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text3"
            android:textSize="20dp"
            android:layout_marginTop="20dp"
            android:id="@+id/my_info_text3" android:layout_below="@+id/my_info_text"
            android:layout_toEndOf="@+id/my_info_img"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:id="@+id/my_info_name"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:textSize="20dp"
            android:paddingLeft="10dp"
            android:background="@drawable/bg_edittext"
            android:layout_marginStart="10dp"
            android:layout_alignBottom="@+id/my_info_text1"
            android:layout_toEndOf="@+id/my_info_text1"
            android:layout_marginEnd="30dp"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20dp"
            android:id="@+id/my_info_nickname"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:background="@drawable/bg_edittext"
            android:layout_marginEnd="30dp"
            android:layout_alignBottom="@+id/my_info_text"
            android:layout_alignStart="@+id/my_info_name"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20dp"
            android:id="@+id/my_info_sex"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:background="@drawable/bg_edittext"
            android:layout_marginEnd="30dp"
            android:layout_alignBottom="@+id/my_info_text3"
            android:layout_alignStart="@+id/my_info_nickname"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text4"
            android:textSize="20dp"
            android:layout_marginStart="15dp"
            android:id="@+id/my_info_text4" android:layout_below="@+id/my_info_img"
            android:layout_alignStart="@+id/my_info_img" android:layout_marginTop="10dp"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text5"
            android:textSize="20dp"
            android:id="@+id/my_info_text5"
            android:layout_marginTop="20dp" android:layout_below="@+id/my_info_text4"
            android:layout_alignStart="@+id/my_info_text4"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text6"
            android:textSize="20dp"
            android:id="@+id/my_info_text6"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/my_info_text5" android:layout_alignEnd="@+id/my_info_text5"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text7"
            android:textSize="20dp"
            android:id="@+id/my_info_text7"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/my_info_text6" android:layout_alignStart="@+id/my_info_text6"/>
        <TextView
            android:textColor="@color/textColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_text8"
            android:textSize="20dp"
            android:layout_marginTop="20dp"
            android:id="@+id/my_info_text8"
            android:layout_below="@+id/my_info_text7" android:layout_alignStart="@+id/my_info_img"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20dp"
            android:id="@+id/my_info_phone"
            android:inputType="number"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/bg_edittext"
            android:layout_alignBottom="@+id/my_info_text4"
            android:layout_toEndOf="@+id/my_info_text4"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20dp"
            android:id="@+id/my_info_wechat"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/bg_edittext"
            android:layout_alignBottom="@+id/my_info_text5"
            android:layout_alignStart="@+id/my_info_phone"/>
        <EditText
            android:textColor="@color/textColor"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20dp"
            android:id="@+id/my_info_qq"
            android:textColorHint="#AAAAAA"
            android:inputType="number"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/bg_edittext"
            android:layout_alignBottom="@+id/my_info_text6"
            android:layout_alignStart="@+id/my_info_wechat"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/good"
            android:id="@+id/my_info_good_img" android:layout_above="@+id/my_info_text8"
            android:layout_alignStart="@+id/my_info_qq"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            android:textSize="20dp"
            android:textColor="@color/top"
            android:id="@+id/my_info_good"
            android:layout_marginStart="10dp"
            android:layout_alignBottom="@+id/my_info_good_img"
            android:layout_toEndOf="@+id/my_info_good_img"/>
        <ImageView
            android:src="@drawable/bad"
            android:layout_marginStart="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/my_info_bad_img" android:layout_above="@+id/my_info_text8"
            android:layout_toEndOf="@+id/my_info_good"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            android:textSize="20dp"
            android:layout_marginStart="10dp"
            android:textColor="#1afa29"
            android:id="@+id/my_info_bad" android:layout_alignBottom="@+id/my_info_bad_img"
            android:layout_toEndOf="@+id/my_info_bad_img"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="80%"
            android:textSize="25dp"
            android:layout_marginStart="25dp"
            android:textColor="#ff5555"
            android:id="@+id/my_info_credit" android:layout_alignBottom="@+id/my_info_bad"
            android:layout_toEndOf="@+id/my_info_bad"/>
        <EditText
            android:textColor="@color/textColor"
            android:lines="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:textColorHint="#AAAAAA"
            android:hint="请输入..."
            android:paddingLeft="10dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/bg_edittext"
            android:id="@+id/my_info_show_me" android:layout_alignTop="@+id/my_info_text8"
            android:layout_alignStart="@+id/my_info_good_img"/>
        <ImageButton
            android:layout_marginTop="10dp"
            android:src="@drawable/pen"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/my_info_change"
            android:layout_below="@+id/my_info_show_me" android:layout_toEndOf="@+id/my_info_bad"/>
        <ImageButton
            android:layout_marginTop="10dp"
            android:src="@drawable/ok"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/my_info_change_ok"
            android:layout_below="@+id/my_info_show_me" android:layout_toEndOf="@+id/my_info_bad"/>
        <ImageButton
            android:layout_marginTop="10dp"
            android:layout_marginStart="20dp"
            android:src="@drawable/exit"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/exit"
            android:layout_below="@+id/my_info_show_me"
            android:layout_toEndOf="@+id/my_info_change"/>
    </RelativeLayout>

</FrameLayout>
